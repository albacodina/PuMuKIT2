{% extends 'PumukitNewAdminBundle:Inbox:base.html.twig' %}

{% block inbox_body %}
{% if is_granted(constant('Pumukit\\SchemaBundle\\Security\\Permission::ACCESS_MULTIMEDIA_SERIES')) %}
<p style="padding: 5px">
  {% trans %}To use the inbox mode define the inbox path as <code>pumukit2.inbox</code> parameter in your config file.{% endtrans %}
